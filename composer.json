{
    "name": "mhamed/spatie-activitylog-browse",
    "description": "Auto-log all models, enrich with request/device data, and browse activity logs via a web UI",
    "keywords": ["laravel", "activitylog", "audit", "browse", "log-viewer", "spatie"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mhamed",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-activitylog": "^4.0",
        "illuminate/support": "^10.0||^11.0||^12.0"
    },
    "autoload": {
        "psr-4": {
            "Mhamed\\SpatieActivitylogBrowse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mhamed\\SpatieActivitylogBrowse\\ActivitylogBrowseServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
